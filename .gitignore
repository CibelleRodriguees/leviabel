lista_numeros = []

maior = 0

menor = 0

for c in range(0,5):

    lista_numeros.append(int(input(f"Digite o numeo para posição {c}: " )))
#reconhecer maior vaçor

if c == 0:
   maior = menor = lista_numeros [c]

else:
     if lista_numeros [c] > maior:
         maior = lista_numeros[c]
     if lista_numeros [c] < menor:
         menor = lista_numeros [c]

print(lista_numeros)

print (f"Maior é {maior}.")

